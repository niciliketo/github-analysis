  <%= form_with model: @search, url: "pull_requests", method: :get do |form| %>
    <%= form.label 'Milestone'%>
    <%= form.select :milestone, @repo.milestones, { include_blank: true }, { multiple: true } %>
    <%= form.label 'Creator' %>
    <%= form.select :creator, @repo.creators, { include_blank: true }, { multiple: true }  %>
    <%= form.label 'Merged By' %>
    <%= form.select :merged_by, @repo.merged_bys, { include_blank: true }, { multiple: true } %>
    <%= form.label 'Start' %>
    <%= form.date_field :pr_merged_at_start %>
    <%= form.label 'End' %>
    <%= form.date_field("pr_merged_at_end") %>
    <%= form.submit "Search" %>

    <%= link_to 'Clear', repo_pull_requests_path %>
 <% end %>



