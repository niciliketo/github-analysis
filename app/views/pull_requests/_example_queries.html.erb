<h4>Params: <%= @search.serialize %></h4>


  <%= form_with model: @search, url: "pull_requests", method: :get do |form| %>
  Milestone:
      <%= form.text_field :milestone %>
  Merged By:
      <%= form.text_field :merged_by %>
  Creator:
      <%= form.text_field :creator %>
  Start:
    <%= form.date_field :pr_merged_at_start %>
  End
    <%= form.date_field("pr_merged_at_end") %>
    <%= form.submit "Search" %>
  <% end %>

