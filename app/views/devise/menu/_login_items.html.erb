<% if user_signed_in? %>
  <li>
  <%= link_to('Logout', destroy_user_session_path, data: { turbo_method: "delete" }) %>        
  </li>
<% else %>
  <li>
  <%= link_to('Login', new_user_session_path) %> 
  </li>
  <li>
  <%= button_to('Login with GitHub', user_github_omniauth_authorize_path, method: :post, data: { turbo: false}) %>  
  </li>
<% end %>